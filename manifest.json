{
	"_version": "1.8.0",
	"sap.app": {
		"id": "ems.UI5Showcase",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ems.UI5Showcase.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ems.UI5Showcase.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ems.UI5Showcase.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "indexTiles",
					"target": [
						"indexTiles"
					]
				},
				{
					"pattern": "vendors",
					"name": "vendors",
					"target": [
						"vendors"
					]
				},
				{
					"pattern": "billingDocs",
					"name": "billingDocs",
					"target": [
						"billingDocs"
					]
				}
			],
			"targets": {
				"indexTiles": {
					"viewName": "IndexTiles",
					"viewId": "indexTiles",
					"viewLevel": 1,
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"async": true,
					"viewPath": "ems.UI5Showcase.view",
					"controlAggregation": "pages",
					"controlId": "app",
					"clearControlAggregation": false
				},
				"vendors": {
					"viewName": "vendors.Vendors",
					"viewType": "XML",
					"viewId": "vendors",
					"viewLevel": 2,
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "ems.UI5Showcase.view",
					"controlAggregation": "pages",
					"controlId": "app",
					"clearControlAggregation": false
				},
				"billingDocs": {
					"viewName": "billing.BillingDocs",
					"viewType": "XML",
					"viewLevel": 2,
					"controlId": "app",
					"viewId": "billingDocs"
				},
				"notFound": {
					"viewType": "XML",
					"viewName": "NotFound"
				}
			}
		}
	}
}